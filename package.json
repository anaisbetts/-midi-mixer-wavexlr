{
  "name": "midi-mixer-plugin-template",
  "prettyName": "Template Plugin",
  "version": "0.1.0",
  "description": "A template plugin for MIDI Mixer.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel src/index.ts --hmr-hostname localhost --public-url . -t browser",
    "build": "tsc && parcel build src/index.ts --public-url . -t browser"
  },
  "settings": {
    "url": {
      "label": "URL",
      "required": true,
      "type": "text"
    },
    "password": {
      "label": "Password",
      "required": true,
      "type": "password"
    }
  },
  "author": "Jack Williams",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "midi-mixer-plugin": "^0.1.5"
  },
  "volta": {
    "node": "14.15.4",
    "npm": "6.14.11"
  }
}
